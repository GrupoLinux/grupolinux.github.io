---
layout: null
---
[
  {% for post in site.posts %}
    {
      "resumo" : "{{ post.resumo }}",
      "data"   : "{{ post.data }}",
      "tag"    : "{{ post.tag }}",
      "by"     : "{{ post.author }}",
      "url"    : "{{ site.baseurl }}{{ post.url }}",
      "titulo" : "{{ post.titulo | jsonify }}"

    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
